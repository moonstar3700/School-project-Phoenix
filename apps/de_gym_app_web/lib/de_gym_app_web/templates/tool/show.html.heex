<h1>Show Tool</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @tool.name %>
  </li>

  <li>
    <strong>Brand:</strong>
    <%= @tool.brand %>
  </li>

  <li>
    <strong>Amount:</strong>
    <%= @tool.amount %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.tool_path(@conn, :edit, @tool) %></span> |
<span><%= link "Back", to: Routes.tool_path(@conn, :index) %></span>
